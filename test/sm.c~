#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int total(int gog,char s[])
 {
    int f,x,h;
    
    f = atoi(&s);
    
    x = gog + 10;
  
    h=malloc(1+x);
  
    printf("%d",sizeof(h));

}

int main(int argc,char **argv)
{
  int p,u,v,t;
  
  char aa[]="Amit"; 
  
  char g,a='d';
 
  p=getchar();
  printf("\n P : %c",p); 
  
  v=total(p,a);

  t = argc<<1;
  calloc(v,10); 
  g=conc(t,aa);

}

